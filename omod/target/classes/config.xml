<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>openmrs-cpm-reviewer</id>
	<name>Concept Proposal Module Reviewer Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.openmrs-cpm-reviewer</package>
	<author>filimonovasvetlana</author>
	<description>
		Concept Proposal Module Reviewer
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/openmrs-cpm-reviewer/update.rdf</updateURL>
	<require_version>1.9.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.openmrs-cpm-reviewer.ConceptProposalModuleReviewerActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.openmrs-cpm-reviewer.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with openmrs-cpm-reviewer. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

